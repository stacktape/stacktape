{
  "name": "stacktape",
  "version": "3.2.0",
  "private": true,
  "bin": {
    "stacktape": "./index.js"
  },
  "engines": {
    "bun": ">=1.3.0"
  },
  "scripts": {
    "dev": "bun run scripts/dev.ts",
    "lint": "bunx eslint ./src ./shared ./scripts ./helper-lambdas --fix",
    "test": "bun test ./src/**/*.spec.ts ./shared/**/*.spec.ts",
    "build:dist": "bun scripts/build-dist-package.ts",
    "build:npm": "bun --max_old_space_size=32768 scripts/build-npm.ts",
    "build:npm:sdk": "bun --max_old_space_size=32768 scripts/build-npm-sdk.ts",
    "build:npm:main": "bun --max_old_space_size=32768 scripts/build-npm-main-export.ts",
    "gen:schema": "bun scripts/generate-schemas.ts",
    "gen:cloudform": "bun scripts/generate-cloudform.ts",
    "gen:sp:metadata": "bun scripts/generate-starter-projects-metadata.ts",
    "gen:res:descriptions": "bun scripts/generate-resource-descriptions.ts",
    "gen:price:info": "bun scripts/generate-static-aws-price-info.ts",
    "gen:sp": "bun scripts/generate-starter-project.ts",
    "gen:db:engines": "bun scripts/generate-db-engine-versions-json.ts",
    "gen:cf:types": "bun scripts/generate-cloudformation-types.ts",
    "gen:pack:builders": "bun scripts/generate-pack-builders-json.ts",
    "gen:ai:docs": "bun scripts/generate-ai-docs.ts",
    "lint:gen:sp": "bun scripts/lint-generated-starter-projects.ts",
    "analyze:bundle": "bun scripts/analyze-bundle.ts",
    "pkg:hl": "bun scripts/package-helper-lambdas.ts",
    "tsc": "node_modules/.bin/tsc --skipLibCheck --noEmit",
    "clean": "node_modules/.bin/rimraf __stacktape-dist __source-maps __publish-folder .stacktape __starter-projects __binary-dist __release __release-npm __platform-packages",
    "docker:clean": "docker system prune --all --force --volumes",
    "validate": "bunx eslint . && bunx tsc --skipLibCheck",
    "reinstall": "node_modules/.bin/rimraf node_modules && bun install",
    "push:gh:repo": "bun scripts/push-to-github-repo.ts",
    "push:starters": "bun scripts/push-starter-projects.ts",
    "compile:cf:schema": "bun scripts/compile-cf-schema.ts",
    "release": "bun scripts/trigger-github-release.ts",
    "build:bin": "bun scripts/trigger-build-binary.ts",
    "publish:schemas": "bun scripts/publish-schemas.ts",
    "publish:install:scripts": "bun scripts/publish-install-scripts.ts",
    "publish:install:scripts:preview": "bun scripts/publish-install-scripts-preview.ts",
    "publish:ai:docs": "bun scripts/publish-ai-docs.ts",
    "esp": "bun scripts/execute-starter-project.ts",
    "run:script": "node",
    "rm:temp": "node_modules/.bin/rimraf .stacktape",
    "publish": "bun run release:gh",
    "upg": "bunx taze",
    "stp": "node_modules/.bin/stacktape"
  },
